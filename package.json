{
	"name": "@goonco/remark-ofm",
	"version": "0.1.0",
	"description": "A remark extension to support obsidian flavored markdown syntax",
	"scripts": {
		"build": "tsup"
	},
	"keywords": [
		"remark",
		"remark-plugin",
		"markdown",
		"ofm"
	],
	"author": {
		"name": "goonco"
	},
	"license": "MIT",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	}
}
